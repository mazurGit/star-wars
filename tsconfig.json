{
  "extends": ["@tsconfig/react-native/tsconfig.json"],
  "compilerOptions": {
    "types": ["react-native"],
    "baseUrl": ".",
    "paths" : {
      "~app/*": ["app/*"],
      },
    "strict": true,
    "noEmit": true
  },
  "noImplicitOverride": true,
  "include": ["app"],
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
